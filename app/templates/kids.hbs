{{outlet}}
<header class = "jumbotron">
  <h1>Activities</h1>
  <h2><a href="kids">Kids </a> | </h2>
  <h2> <a href="adults"> 21 and Over</a></h2>
</header>


<h2>kids</h2>

{{new-activity categories=model.categories save = "save"}}

<ul>
  {{#each model.activities as |activity| }}
    {{#if activity.kid }}
      <li>{{activity.name}}</li>
      <img src="{{activity.image}}" class = "activity-img" />
    {{/if}}
  {{/each}}
</ul>
